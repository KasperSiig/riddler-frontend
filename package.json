{
	"name": "riddler-frontend",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"buildprod": "ng build --prod",
		"test": "jest --coverage",
		"test:watch": "jest --watch",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"codecov": "codecov"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.11",
		"@angular/common": "~8.2.11",
		"@angular/compiler": "~8.2.11",
		"@angular/core": "~8.2.11",
		"@angular/forms": "~8.2.11",
		"@angular/platform-browser": "~8.2.11",
		"@angular/platform-browser-dynamic": "~8.2.11",
		"@angular/router": "~8.2.11",
		"@ngxs/router-plugin": "^3.5.1",
		"@ngxs/store": "^3.5.1",
		"jest-preset-angular": "^8.0.0",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-builders/jest": "^8.3.0",
		"@angular-devkit/build-angular": "~0.803.13",
		"@angular/cli": "~8.3.13",
		"@angular/compiler-cli": "~8.2.11",
		"@angular/language-service": "~8.2.11",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/jest": "^24.0.23",
		"@types/node": "~8.9.4",
		"codecov": "^3.6.1",
		"codelyzer": "^5.0.0",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"jest": "^24.9.0",
		"prettier": "^1.18.2",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setupJest.ts"
		],
		"globals": {
			"ts-jest": {
				"tsConfig": "<rootDir>/tsconfig.spec.json",
				"stringifyContentPathRegex": "\\.html$",
				"astTransformers": [
					"<rootDir>/node_modules/jest-preset-angular/build/InlineFilesTransformer",
					"<rootDir>/node_modules/jest-preset-angular/build/StripStylesTransformer"
				]
			}
		}
	}
}
